{
  "name": "culture-connect-backend",
  "version": "1.0.0",
  "description": "Backend for Culture Connect application",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "sync:db": "node src/scripts/syncDatabase.js",
    "seed:activities": "node src/scripts/seedActivities.js",
    "seed:mock": "node src/scripts/seedMockData.js",
    "seed": "npm run seed:activities && npm run seed:mock",
    "reset:db": "npm run sync:db && npm run seed"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.35.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
