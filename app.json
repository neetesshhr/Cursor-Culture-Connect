{
  "expo": {
    "name": "TRiVO",
    "slug": "bolt-expo-nativewind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "scheme": "myapp",
    "owner": "rijalboy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.babish.dev.boltexponativewind"
    },
    "android": {
      "package": "com.babish.dev.boltexponativewind"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-4289098365837690~2373905837",
          "iosAppId": "ca-app-pub-4289098365837690/4760411931"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "fecb2cb4-c2cd-4536-a6c8-fea3c961c70c"
      }
    }
  }
}
